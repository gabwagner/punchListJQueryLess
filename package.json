{
  "name": "punchlist",
  "version": "1.0.11",
  "description": "Punch List JQueryLess",
  "scripts": {
    "dev": "copyfiles -u 3 node_modules/autosize/dist/autosize.js src/",
    "build": "npm run buildMin && npm run buildDist && npm run buildExample",
    "buildMin": "webpack",
    "buildExample": "copyfiles -u 1 dist/*.js example/js && copyfiles -u 1 dist/*.css example/css",
    "buildDist": "copyfiles -u 1 src/* dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabwagner/punchListJQueryLess.git"
  },
  "keywords": [
    "punchlist",
    "html"
  ],
  "author": "gwagner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabwagner/punchListJQueryLess/issues"
  },
  "homepage": "https://github.com/gabwagner/punchListJQueryLess#readme",
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "autosize": "^4.0.2"
  }
}
